CREATE OR REPLACE TABLE my_db.main.ca_candidate_project_data AS
SELECT
    -- already DATE, just keep it
    CAST(MONTH AS DATE) AS MONTH,

    -- text dimensions
    CAST(ORIGIN AS TEXT)        AS ORIGIN,
    CAST(ORIG_STATE AS TEXT)    AS ORIG_STATE,
    CAST(DESTINATION AS TEXT)   AS DESTINATION,
    CAST(DEST_STATE AS TEXT)    AS DEST_STATE,
    CAST(AIRCRAFT AS TEXT)      AS AIRCRAFT,

    -- integers
    TRY_CAST(FLIGHTS AS INTEGER)               AS FLIGHTS,
    TRY_CAST(FLIGHT_HOURS AS INTEGER)          AS FLIGHT_HOURS,
    TRY_CAST(PASSENGERS AS INTEGER)            AS PASSENGERS,
    TRY_CAST(FIRST_CLASS_PAX AS INTEGER)       AS FIRST_CLASS_PAX,
    TRY_CAST(PREMIUM_ECONOMY_PAX AS INTEGER)   AS PREMIUM_ECONOMY_PAX,
    TRY_CAST(ECONOMY_PAX AS INTEGER)           AS ECONOMY_PAX,
    TRY_CAST(ASMS AS INTEGER)                  AS ASMS,
    TRY_CAST(SEATS AS INTEGER)                 AS SEATS,
    TRY_CAST("FIRST_CLASS SEATS" AS INTEGER)   AS "FIRST_CLASS SEATS",
    TRY_CAST(PREM_ECONOMY_SEATS AS INTEGER)    AS PREM_ECONOMY_SEATS,
    TRY_CAST(ECONOMY_SEATS AS INTEGER)         AS ECONOMY_SEATS,
    TRY_CAST(FREIGHT_LBS AS INTEGER)           AS FREIGHT_LBS,

    -- doubles
    TRY_CAST(MILES_DISTANCE AS DOUBLE)             AS MILES_DISTANCE,
    TRY_CAST(FIRST_CLASS_REVENUE AS DOUBLE)        AS FIRST_CLASS_REVENUE,
    TRY_CAST(PREM_ECONOMY_REVENUE AS DOUBLE)       AS PREM_ECONOMY_REVENUE,
    TRY_CAST(ECONOMY_REVENUE AS DOUBLE)            AS ECONOMY_REVENUE,
    TRY_CAST(BAGGAGE_REVENUE AS DOUBLE)            AS BAGGAGE_REVENUE,
    TRY_CAST(OTHER_ANCILLARY_REVENUE AS DOUBLE)    AS OTHER_ANCILLARY_REVENUE,
    TRY_CAST(FREIGHT_REVENUE AS DOUBLE)            AS FREIGHT_REVENUE,
    TRY_CAST(AIRCRAFT_OPERATION_COST AS DOUBLE)    AS AIRCRAFT_OPERATION_COST,
    TRY_CAST(AIRPORT_FEE_COST AS DOUBLE)           AS AIRPORT_FEE_COST,
    TRY_CAST(CREW_AND_LABOR_COST AS DOUBLE)        AS CREW_AND_LABOR_COST,
    TRY_CAST(FUEL_COST AS DOUBLE)                  AS FUEL_COST
FROM my_db.main.ca_candidate_project_data;
